{
  "name": "carcel-wishlist",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "remix dev",
    "generate:css": "npx tailwindcss -o ./app/styles/tailwind.css",
    "migrate": "node-pg-migrate",
    "ntl:ci": "netlify dev",
    "start": "cross-env NODE_ENV=production netlify dev",
    "eslint": "eslint '**/*.{ts,tsx}'",
    "prettier": "prettier '**/*.{ts,tsx}'",
    "stylelint": "stylelint '**/*.css'",
    "typecheck": "tsc -b"
  },
  "dependencies": {
    "@netlify/functions": "^1.3.0",
    "@remix-run/netlify": "^1.8.2",
    "@remix-run/node": "^1.8.2",
    "@remix-run/react": "^1.8.2",
    "bcryptjs": "^2.4.3",
    "cross-env": "^7.0.3",
    "node-pg-migrate": "^6.2.2",
    "pg": "^8.8.0",
    "postcss": "^8.4.20",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.2.4",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.8.2",
    "@remix-run/eslint-config": "^1.8.2",
    "@remix-run/serve": "^1.8.2",
    "@testing-library/cypress": "^9.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/eslint": "^8.4.10",
    "@types/node": "^18.11.15",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "cypress": "^12.1.0",
    "dot-prop": "^7.2.0",
    "eslint": "^8.29.0",
    "netlify-cli": "^13.0.0",
    "netlify-plugin-cypress": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.1",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "stylelint": "^15.2.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^30.0.1",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=14"
  }
}
